        if (fname == "" || email == "" || profile == "") {
            alert('Please fill all the fields')
            return false;
        }
        {
            if (!validateFName(fname)) {
                alert('Please enter valid First Name');
                return false;
            }
            else if (!validateLName(lname)) {
                alert('Please enter valid Last Name');
                return false;
            }
            if (!validateEmail(email)) {
                alert('not valid email');
                return false;
            }
            else {
                $.ajax({
                    url: "../Index/SendCareerMail?fname=" + fname + "&lname=" + lname + "&email=" + email + "&Subject=" + Subject + "&profile=" + profile,
                    type: "post",
                    data: fd,
                    processData: false,
                    contentType: false,
                    success: function (msg) {
                        debugger;
                        if (msg == "Success") {
                            alertify.success('Mail has been sent successfully!');
                            $('#fname').val('');
                            $('#lname').val('');
                            $('#emailid').val('');
                            $('#profile').val('');
                            $('#fileuploadText').val('');
                        }
                        else {
                            alertify.error("failure");
                            return false;
                        }
                    },
                    error: function () {
                        alert("failure");
                    }
                });
            }
        }
    }
    function validateEmail($email) {
        var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
        if (!emailReg.test($email)) {
            return false;
        } else {
            return true;
        }
    }
    function validateFName($name) {
        var fnameReg = /^[A-Z,a-z._]+$/;
        if (!fnameReg.test($name)) {
            return false;
        } else {
            return true;
        }
    }
    function validateLName($lname) {
        var lnameReg = /^[A-Z,a-z._]+$/;
        if (!lnameReg.test($lname)) {
            return false;
        } else {
            return true;
        }
    }

    function ResetAll() {
        $('#fname').val('');
        $('#lname').val('');
        $('#emailid').val('');
        $('#profile').val('');
        $('#fileuploadText').val('');
    }

</script>



