<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="TeamMemberProfile" table="TeamMemberProfile" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="TeamId">
      <column name="TeamId" />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>
    <property name="ProfileType">
      <column name="ProfileType"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>
    <property name="StatusUpdateDate">
      <column name="StatusUpdateDate"  />
    </property>

    <property name="ProfilePicUrl">
      <column name="ProfilePicUrl"  />
    </property>
    <property name="ProfileName">
      <column name="ProfileName"  />
    </property>

  </class>
</hibernate-mapping>