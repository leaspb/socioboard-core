<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">
  <class name="DropboxAccount" table="DropboxAccount" lazy="true" >
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="DropboxUserId">
      <column name="DropboxUserId" />
    </property>
    <property name="DropboxUserName">
      <column name="DropboxUserName" />
    </property>
    <property name="DropboxEmail">
      <column name="DropboxEmail" />
    </property>
    <property name="OauthToken">
      <column name="OauthToken" />
    </property>
    <property name="OauthTokenSecret">
      <column name="OauthTokenSecret" />
    </property>
    <property name="AccessToken">
      <column name="AccessToken" />
    </property>
    <property name="CreateDate">
      <column name="CreateDate" />
    </property>
  </class>
</hibernate-mapping>