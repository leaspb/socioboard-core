<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="FacebookMessage" table="FacebookMessage" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="Message">
      <column name="Message" />
    </property>
    <property name="MessageDate">
      <column name="MessageDate"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>


    <property name="FromId">
      <column name="FromId"  />
    </property>

    <property name="FromName">
      <column name="FromName"  />
    </property>

    <property name="FromProfileUrl">
      <column name="FromProfileUrl"  />
    </property>

    <property name="FbComment">
      <column name="FbComment"  />
    </property>


    <property name="FbLike">
      <column name="FbLike"  />
    </property>

    <property name="MessageId">
      <column name="MessageId"  />
    </property>
    <property name="Type">
      <column name="Type"  />
    </property>

    <property name="UserId">
      <column name="UserId"  />
    </property>

    <property name="Picture">
      <column name="Picture"  />
    </property>
    <property name="IsArchived">
      <column name="IsArchived"  />
    </property>

  </class>
</hibernate-mapping>
