<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">
  <class name="Invitation" table="Invitation" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    
    <property name="SenderEmail">
      <column name="SenderEmail"  />
    </property>
    
    <property name="SenderUserId">
      <column name="SenderUserId"  />
    </property>
    
    <property name="FriendEmail">
      <column name="FriendEmail"  />
    </property>
    
    <property name="FriendUserId">
      <column name="FriendUserId"  />
    </property>

    <property name="InvitationCode">
      <column name="InvitationCode"  />
    </property>

    <property name="SendInvitationDate">
      <column name="SendInvitationDate"  />
    </property>
   
    <property name="AcceptInvitationDate">
      <column name="AcceptInvitationDate"  />
    </property>
      
    <property name="Status">
      <column name="Status"  />
    </property>
    

  </class>
</hibernate-mapping>
