<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="FbPageComment" table="FbPageComment" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="PostId">
      <column name="PostId"  />
    </property>
    <property name="FromId">
      <column name="FromId"  />
    </property>
    <property name="FromName">
      <column name="FromName"  />
    </property>
    <property name="CommentId">
      <column name="CommentId"  />
    </property>
    <property name="Comment">
      <column name="Comment"  />
    </property>
    <property name="PictureUrl">
      <column name="PictureUrl"  />
    </property>
    <property name="Likes">
      <column name="Likes"  />
    </property>
    <property name="UserLikes">
      <column name="UserLikes"  />
    </property>
    <property name="Commentdate">
      <column name="Commentdate"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    

  </class>
</hibernate-mapping>

