<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="TicketAssigneeStatus" table="TicketAssigneeStatus" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="AssigneeUserId">
      <column name="AssigneeUserId" />
    </property>
    <property name="AssignedTicketCount">
      <column name="AssignedTicketCount"  />
    </property>
  </class>
</hibernate-mapping>