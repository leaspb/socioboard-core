<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="InboxMessages" table="InboxMessages" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="MessageId">
      <column name="MessageId" />
    </property>
    <property name="ProfileId">
      <column name="ProfileId" />
    </property>
    <property name="FromId">
      <column name="FromId"  />
    </property>
    <property name="FromName">
      <column name="FromName"  />
    </property>
    <property name="RecipientId">
      <column name="RecipientId"  />
    </property>
    <property name="RecipientName">
      <column name="RecipientName"  />
    </property>
    <property name="Message">
      <column name="Message"  />
    </property>
    <property name="FromImageUrl">
      <column name="FromImageUrl"  />
    </property>
    <property name="RecipientImageUrl">
      <column name="RecipientImageUrl"  />
    </property>
    <property name="ProfileType">
      <column name="ProfileType"  />
    </property>
    <property name="MessageType">
      <column name="MessageType"  />
    </property>
    <property name="CreatedTime">
      <column name="CreatedTime"  />
    </property>
    <property name="EntryTime">
      <column name="EntryTime"  />
    </property>
    <property name="FollowerCount">
      <column name="FollowerCount"  />
    </property>
    <property name="FollowingCount">
      <column name="FollowingCount"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>
  </class>
</hibernate-mapping>