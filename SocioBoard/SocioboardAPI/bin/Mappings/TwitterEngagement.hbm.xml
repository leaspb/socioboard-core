<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Domain"
      assembly="Domain.Socioboard">

  <class name="TwitterEngagement" table="TwitterEngagement" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="ProfileId">
      <column name="ProfileId" />
    </property>
    <property name="Engagement">
      <column name="Engagement"  />
    </property>
    <property name="RetweetCount">
      <column name="RetweetCount"  />
    </property>
    <property name="ReplyCount">
      <column name="ReplyCount"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="Influence">
      <column name="Influence"  />
    </property>
    
  </class>
</hibernate-mapping>